{
  "$schema": "https://raw.githubusercontent.com/rescript-lang/rescript-compiler/master/docs/docson/build-schema.json",
  "name": "rescript-project-template",
  "version": "0.0.1",
  "reason": {
    "react-jsx": 3
  },
  "bsc-flags": [
    "-bs-super-errors"
  ],
  "sources": {
    "dir": "src",
    "subdirs": true
  },
  "package-specs": {
    "module": "es6",
    "in-source": false
  },
  "suffix": ".bs.js",
  "namespace": true,
  "bs-dependencies": [
    "reason-react"
  ],
  "warnings": {
    "error": "+101"
  },
  "ppx-flags": [],
  "refmt": 3
}
